{
  "name": "@ibimina/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.public.ts",
  "files": [
    "dist",
    "tokens.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --max-warnings=0 .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run test:unit",
    "test:unit": "pnpm exec c8 --clean=false --temp-directory=.nyc_output --report-dir=coverage/unit --reporter=lcov --reporter=json-summary --reporter=text-summary tsx --test tests/unit/*.test.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.public.ts",
      "import": "./src/index.public.ts",
      "require": "./src/index.public.ts",
      "default": "./src/index.public.ts"
    },
    "./index.public": "./src/index.public.ts",
    "./tokens.json": "./tokens.json",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/node": "20.19.21",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.11",
    "@types/react-native": "^0.73.0",
    "expo-blur": "^15.0.7",
    "expo-linear-gradient": "^15.0.7",
    "lucide-react": "0.545.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.76.6",
    "react-native-reanimated": "^3.16.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "expo-blur": "^15.0.0",
    "expo-linear-gradient": "^15.0.0",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.76.0",
    "react-native-reanimated": "^3.0.0"
  }
}
