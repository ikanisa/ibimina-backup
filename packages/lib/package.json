{
  "name": "@ibimina/lib",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "build": "pnpm run clean && tsup src/index.ts --format cjs,esm && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "node ../../config/tooling/scripts/run-eslint.mjs ./src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --config vitest.config.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.public.ts",
      "import": "./src/index.public.ts",
      "require": "./src/index.public.ts",
      "default": "./src/index.public.ts"
    },
    "./index.public": "./src/index.public.ts",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "posthog-node": "^4.3.0"
  },
  "devDependencies": {
    "@sentry/types": "^8.39.0",
    "@types/node": "20.19.21",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
