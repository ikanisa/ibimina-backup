{
  "name": "@ibimina/flags",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./src/index.public.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.public.ts",
      "import": "./src/index.public.ts",
      "require": "./src/index.public.ts",
      "default": "./src/index.public.ts"
    },
    "./index.public": "./src/index.public.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsup src/index.ts --format cjs,esm && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@ibimina/config": "workspace:*",
    "@supabase/supabase-js": "^2.78.0"
  },
  "devDependencies": {
    "@types/node": "20.19.21",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.9.3"
  }
}
